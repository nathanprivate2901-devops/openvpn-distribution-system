{
  "name": "openvpn-distribution-system",
  "version": "1.0.0",
  "description": "Backend system for managing OpenVPN configuration distribution with user authentication, QoS policies, and Docker container management",
  "main": "src/index.js",
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "echo \"Error: no test specified\" && exit 0",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "openvpn",
    "vpn",
    "authentication",
    "express",
    "nodejs",
    "docker",
    "qos",
    "backend"
  ],
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js,json",
    "ignore": [
      "src/**/*.test.js",
      "logs/*"
    ],
    "delay": 1000
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/openvpn-distribution-system.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/openvpn-distribution-system/issues"
  },
  "homepage": "https://github.com/yourusername/openvpn-distribution-system#readme"
}
